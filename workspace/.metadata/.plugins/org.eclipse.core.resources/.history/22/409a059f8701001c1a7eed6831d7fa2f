package ch05;

//05-01. for반복문
public class ForMainTest3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		// 중첩반복문
		

		for(int i = 2; i<=9; i++) {
			System.out.println(i + "단 입니다.");
			for(int j = 1; j<=9; j++) {
				System.out.println(i + " * " + j + " = " + (i*j));
			}
			System.out.println("---------------------------------");
		}
		
	}

}
