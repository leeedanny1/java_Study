package ch03;

public class Teacher extends Person {
	
	String name;
	int age;
	int height;
	int weight;
	
	// 持失切
	public Teacher() {
		name = "畠識持";
		age = 37;
		height = 180;
		weight = 70;
	}
	
	//五社球
	public void showInfo() {
		System.out.println("name: " + name);
		System.out.println("age: " + age);
		System.out.println("height: " + height);
		System.out.println("weight: " + weight);	
	}

}
