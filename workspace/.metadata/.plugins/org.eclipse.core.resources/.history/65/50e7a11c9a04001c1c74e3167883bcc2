package ch02;

public class ForMainTest1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		// 순차 증가하는 별(*)모양 만들기
		
		
		for(int i = 1; i<6; i++) {
			System.out.println(" ");
			for(int j = 0; j<i; j++) {
				System.out.print("*");
			}
			System.out.println();
		}

	}

}
