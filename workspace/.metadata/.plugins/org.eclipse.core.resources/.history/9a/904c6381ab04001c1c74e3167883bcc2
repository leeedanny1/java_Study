package ch04;

public class Subway {
	int lineNumber;
	int passengerCount;
	int money;
	
	
	public Subway(int lineNumber) {
		this.lineNumber = lineNumber;
	}
	
	
	// 메소드
	public void take(int money) {
		this.money = money;
		this.passengerCount ++;
	}
	
	public void showInfo() {
		System.out.println(busNo + "번의 승객은");
		System.out.println(passengerCount + "명 이고");
		System.out.println("수입금은 " + money + "원 입니다.");		
	}
	

}
