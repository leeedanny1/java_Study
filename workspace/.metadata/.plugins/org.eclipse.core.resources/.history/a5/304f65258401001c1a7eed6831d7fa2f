package ch05;

//05-01. for반복문
public class ForMainTest2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num=2;
		
		System.out.println("구구단 프로그램입니다. \nnum단을 선택하셨습니다.");
		for(int i=1; i<=9; i++) {
			System.out.println(num + "X" + i + "=" + (num*i));
		}

	}

}
