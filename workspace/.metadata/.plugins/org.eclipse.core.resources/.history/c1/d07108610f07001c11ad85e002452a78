package ch03;

public class Student {

	String name;
	int age;
	int height;
	int weight;
	
	// 생성자
	public Student() {
		name = "홍길동";
		age = 21;
		height = 175;
		weight = 65;
	}
	
	//메소드
	public void showInfo() {
		System.out.println("name: " + name);
		System.out.println("age: " + age);
		System.out.println("height: " + height);
		System.out.println("weight: " + weight);	
	}
	
	
	
	
}
