package ch05;

public class ForMainTest4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int num=9;
		
		for(int i=0; i<num; i++ ) {
			for(int j=i; j<num-1; j++) {
				System.out.print(" ");
			}
			for(int j=num; j<i; j--) {
				System.out.print("*");
			}
		}
		
		
	}

}
