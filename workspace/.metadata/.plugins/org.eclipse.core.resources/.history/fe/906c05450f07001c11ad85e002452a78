package ch03;

public class Person {
	
	String name;
	int age;
	int height;
	int weight;
	
	
	public Person(String name, int age, int height, int weight) {
		this.name = name;
		this.age = age;
		this.height = height;
		this.weight = weight;
	}
	
	//¸Þ¼Òµå
		public void showInfo() {
			System.out.println("name: " + name);
			System.out.println("age: " + age);
			System.out.println("height: " + height);
			System.out.println("weight: " + weight);	
		}

}
